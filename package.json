{
  "name": "neverendingstickies",
  "version": "1.0.0",
  "description": "Virutal stickies. Hierarchical, stickies can have never ending stickies attched to them.",
  "main": "index.js",
  "scripts": {
    "test": "NODE_OPTIONS=\"-r @std/esm --experimental-modules\" tape tests/**/*.mjs | node_modules/.bin/tap-spec",
    "test:one": "NODE_OPTIONS=\"-r @std/esm --experimental-modules\" nodemon tape",
    "start": "http-server ./dist",
    "build": "webpack -d"
  },
  "keywords": [
    "stickies",
    "agile",
    "kanban",
    "boards",
    "online",
    "notes"
  ],
  "author": "Joey Guerra",
  "license": "ISC",
  "dependencies": {
    "@std/esm": "^0.26.0",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "jsdom": "^13.0.0",
    "jsdom-devtools-formatter": "^1.0.0",
    "nodemon": "^1.18.6",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.1",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2"
  },
  "@std/esm": "cjs",
  "nodemonConfig": {
    "ignore": [
      "tests/*",
      "data/*"
    ]
  }
}
